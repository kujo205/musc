{
  "name": "yt-music-sharer",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "codegen": "prisma db pull --schema=src/server/db/types/schema.prisma && prisma-case-format --file src/server/db/types/schema.prisma && prisma generate --schema=src/server/db/types/schema.prisma  && prettier --write src/server/db/types/db-types.d.ts src/server/db/types/enums.ts",
    "test": "npm run test:unit && npm run test:i && npm run test:e2e",
    "test:unit": "vitest --coverage --config ./tests/conf/vite.unit.config.ts",
    "pretest:i": "dotenv -e .env.test npm run db:up && dotenv -e .env.test npm run db:migrate && dotenv -e .env.test npm run db:seed",
    "test:i": "vitest --coverage  --config ./tests/conf/vite.integration.config.ts",
    "posttest:i": "dotenv -e .env.test npm run db:down",
    "test:e2e": "playwright test",
    "db:make-migration": "bunx kysely migrate:make",
    "db:migrate": "npx kysely migrate:latest",
    "db:make-seed": "npx kysely seed:make",
    "db:seed": "npx kysely seed:run",
    "db:up": "docker compose up -d db",
    "db:down": "docker compose down db",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.3",
    "@sveltejs/adapter-node": "^5.2.9",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/svelte": "^5.2.6",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint": "^9.6.0",
    "@vitest/coverage-istanbul": "^2.1.5",
    "@vitest/coverage-v8": "2.1.8",
    "autoprefixer": "^10.4.20",
    "bits-ui": "^1.0.0-next.69",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^7.4.4",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "formsnap": "^2.0.0-next.1",
    "globals": "^15.0.0",
    "happy-dom": "^15.11.6",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "kysely": "^0.26.1",
    "kysely-ctl": "^0.9.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prisma": "^5.22.0",
    "prisma-case-format": "1.7.3",
    "prisma-kysely": "^1.8.0",
    "sass-embedded": "^1.82.0",
    "svelte": "5.2.8",
    "svelte-check": "^4.0.0",
    "svelte-sonner": "^0.3.28",
    "sveltekit-superforms": "^2.21.1",
    "tailwind-merge": "^2.5.4",
    "tailwind-variants": "^0.2.1",
    "tailwindcss": "^3.4.9",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.7.2",
    "typescript-eslint": "^8.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.0.4",
    "zod": "^3.23.8"
  },
  "dependencies": {
    "@auth/kysely-adapter": "^1.7.4",
    "@auth/sveltekit": "^1.7.4",
    "lucide-svelte": "^0.468.0",
    "mode-watcher": "^0.5.0",
    "mysql2": "^3.11.4",
    "vite": "5.4.11"
  },
  "lint-staged": {
    "*.{js,ts,svelte}": "eslint --cache --fix",
    "*.{js,ts,svelte,css,md}": "prettier --write"
  }
}
